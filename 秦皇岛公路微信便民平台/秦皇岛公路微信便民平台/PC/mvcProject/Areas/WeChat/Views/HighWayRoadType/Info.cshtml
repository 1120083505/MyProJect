@{ 
    Layout = "~/Views/Shared/_Index_Create.cshtml"; 
} 
@model  ZrSoft.WeChat_HighWayRoadType 
<script src="~/lib/angular.js"></script> 
<script> 
    layui.use(['form', 'layedit', 'laydate'], function () { 
        var form = layui.form() 
        , layer = layui.layer; 
    }); 
</script> 
<fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;"> 
    <legend> 
        <a class="layui-icon close" style="padding: 5px; font-size:larger; cursor:pointer" title="@Resource.lang.Back" >&#xe6ff;</a>  @Resource.lang.WeChat_HighWayRoadType</legend> 
</fieldset> 
<form class="layui-form layui-form-pane" action=""> 
    <div class="layui-form-item"> 
        <label class="layui-form-label">@Resource.lang.WeChat_HighWayRoadType_Name</label><div class="layui-input-block"> 
                    <label name="@Model.Name" class="layui-input">@Model.Name</label> 
        </div>
    </div> 
    <div class="layui-form-item"> 
        <label class="layui-form-label">@Resource.lang.WeChat_HighWayRoadType_Type</label><div class="layui-input-block"> 
                    <label name="@Model.Type" class="layui-input">@Model.Type</label> 
        </div>
    </div> 
    <div class="layui-form-item"> 
        <label class="layui-form-label">@Resource.lang.WeChat_HighWayRoadType_AdminId</label><div class="layui-input-block"> 
                    <label name="@Model.AdminId" class="layui-input">@Model.AdminId</label> 
        </div>
    </div> 
    <div class="layui-form-item"> 
        <label class="layui-form-label">@Resource.lang.WeChat_HighWayRoadType_PubDate</label><div class="layui-input-block"> 
                    <label name="@Model.PubDate" class="layui-input">@Model.PubDate</label> 
        </div>
    </div> 
    <div class="layui-form-item"> 
        <label class="layui-form-label">@Resource.lang.WeChat_HighWayRoadType_EditAdminId</label><div class="layui-input-block"> 
                    <label name="@Model.EditAdminId" class="layui-input">@Model.EditAdminId</label> 
        </div>
    </div> 
    <div class="layui-form-item"> 
        <label class="layui-form-label">@Resource.lang.WeChat_HighWayRoadType_EditPubDate</label><div class="layui-input-block"> 
                    <label name="@Model.EditPubDate" class="layui-input">@Model.EditPubDate</label> 
        </div>
    </div> 
    <div class="layui-form-item"> 
        <label class="layui-form-label">@Resource.lang.WeChat_HighWayRoadType_Memo</label><div class="layui-input-block"> 
                    <label name="@Model.Memo" class="layui-input">@Model.Memo</label> 
        </div>
    </div> 
</form> 
