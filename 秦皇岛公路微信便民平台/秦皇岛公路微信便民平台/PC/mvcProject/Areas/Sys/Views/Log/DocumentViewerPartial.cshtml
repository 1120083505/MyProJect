@using DevExpress.Web.Mvc.UI;
@using DevExpress.XtraReports.Web.DocumentViewer;
@Html.DevExpress().DocumentViewer(settings =>
{
    settings.Name = "DocumentViewer";
    settings.CallbackRouteValues = new { Controller = ViewContext.RouteData.Values["controller"], Action = "DocumentViewerPartial" };
    settings.ExportRouteValues = new { Controller = ViewContext.RouteData.Values["controller"], Action = "DocumentViewerPartialExport" };
    settings.Report = (DevExpress.XtraReports.UI.XtraReport)Model;
    settings.ToolbarMode = DocumentViewerToolbarMode.StandardToolbar;
}).GetHtml()